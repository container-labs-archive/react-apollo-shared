{
  "name": "@containerlabs/react-apollo-shared",
  "version": "0.6.0",
  "description": "a shared library for hocs and utils used in a React / Apollo application",
  "main": "functions/index.js",
  "scripts": {
    "package-functions-production": "funpack -e production",
    "package-functions-staging": "funpack -e staging",
    "package-functions-local": "funpack -e local",
    "ppare-deploy": "npm run package-functions-production",
    "predeploy": "npm run ppare-deploy",
    "deploy": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/container-labs/react-apollo-shared.git"
  },
  "keywords": [
    "React",
    "Apollo",
    "graphql",
    "higher-order-component"
  ],
  "author": "Will Beebe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/container-labs/react-apollo-shared/issues"
  },
  "homepage": "https://github.com/container-labs/react-apollo-shared#readme",
  "dependencies": {
    "lodash": "latest",
    "material-ui": "1.0.0-beta.23",
    "material-ui-icons": "^1.0.0-beta.17",
    "react": "15.6.2",
    "react-redux": "5.0.5",
    "react-router-redux": "^5.0.0-alpha.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.31",
    "@babel/core": "^7.0.0-beta.31",
    "@babel/preset-env": "7.0.0-beta.31",
    "@containerlabs/funpack": "^0.8.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^7.0.0-beta.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-imports": "^1.4.1",
    "babel-plugin-transform-inline-environment-variables": "^0.2.0",
    "babel-plugin-transform-react-constant-elements": "^7.0.0-beta.3",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-react-remove-prop-types": "0.4.4",
    "babel-plugin-transform-runtime": "^7.0.0-beta.3",
    "babel-preset-flow": "^7.0.0-beta.3",
    "babel-preset-react": "^7.0.0-beta.3",
    "babel-preset-stage-2": "^7.0.0-beta.3",
    "babel-register": "^7.0.0-beta.3",
    "react-decoration": "^1.4.1"
  },
  "peerDependenciers": {
    "firebase-admin": "5.2.1"
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "node": "6.11.1"
          }
        }
      ],
      "stage-2",
      "flow",
      "react"
    ],
    "plugins": [
      "add-module-exports",
      "transform-decorators-legacy",
      "transform-runtime",
      "transform-imports",
      "transform-inline-environment-variables",
      "transform-react-jsx",
      "transform-class-properties",
      "transform-flow-strip-types",
      "react-html-attrs"
    ]
  }
}
